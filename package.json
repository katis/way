{
  "name": "@katis/way",
  "version": "3.0.0",
  "description": "Create type-safe URL path builders using Proxies",
  "author": "Joni Katajam√§ki",
  "license": "ISC",
  "keywords": [
    "typed",
    "ts",
    "path",
    "builder",
    "url",
    "router",
    "route",
    "proxy"
  ],
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "tsup --dts --format esm,cjs src/index.ts",
    "test": "vitest"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.1",
    "@types/qs": "^6.9.8",
    "@vitest/coverage-v8": "^0.34.6",
    "query-string": "^8.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "zod": "^3.22.4"
  }
}
